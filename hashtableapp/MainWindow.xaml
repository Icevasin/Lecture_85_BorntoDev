<Window x:Class="hashtableapp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hashtableapp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Center" Height="31" Margin="0,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="592" Text="key"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Center" Height="31" Margin="0,172,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="592" Text="value"/>
        <Button x:Name="btnRemove" Content="Remove" HorizontalAlignment="Left" Height="66" Margin="313,228,0,0" VerticalAlignment="Top" Width="177" Click="btnRemove_Click"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Height="66" Margin="104,228,0,0" VerticalAlignment="Top" Width="177" Click="btnAdd_Click"/>
        <Button x:Name="btnShow" Content="Show All" HorizontalAlignment="Left" Height="66" Margin="519,228,0,0" VerticalAlignment="Top" Width="177" Click="btnShow_Click"/>

    </Grid>
</Window>
